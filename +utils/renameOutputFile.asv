function filepath = renameOutputFile(folder, filename)
%RENAMEOUTPUTFILE Summary of this function goes here
%   Detailed explanation goes here

[~, name, ext] = fileparts(filename);

hIdx = strfind(name, '1-');
bIdx = strfind(name, '_');

filepath = fullfile( ...
    folder, ...
    strcat( ...
        name(hIdx(1)+2:bIdx(1)-1), 
    )
end

